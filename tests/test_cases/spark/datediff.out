SELECT
  DATE_DIFF(
    'day',
    CAST(SUBSTR(CAST(TRY_CAST('2023-01-01' AS DATE) AS VARCHAR), 1, 10) AS DATE),
    CAST(SUBSTR(CAST(TRY_CAST('2024-01-01' AS DATE) AS VARCHAR), 1, 10) AS DATE)
  )